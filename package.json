    "chai-as-promised": "^5.1.0",
    "ember-cli": "2.8.0",
    "ember-cli-release": "^0.2.9",
    "eslint": "^3.18.0",
    "mocha": "^2.3.3"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "dependencies": {
    "aws-sdk": "^2.6.0",
    "core-object": "^2.0.6",
    "ember-cli-babel": "^5.1.6",
    "ember-cli-deploy-plugin": "^0.2.6",
    "rsvp": "^3.2.1"
  },
  "ember-addon": {
