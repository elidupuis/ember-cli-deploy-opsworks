{
  "name": "ember-cli-deploy-opsworks",
  "version": "0.1.0",
  "description": "Ember CLI Deploy plugin to trigger a new deployment on ASW OpsWorks.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "node tests/runner.js"
  },
  "repository": {
